{
  "name": "limu-paste",
  "displayName": "limu-paste",
  "description": "快速复制粘贴源文件",
  "version": "0.0.4",
  "publisher": "leixiang",
  "engines": {
    "vscode": "^1.72.2"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onCommand:limu-paste"
  ],
  "contributes": {
    "commands": [
      {
        "command": "limu-paste",
        "title": "limu-paste"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "limu-paste",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "command": "limu-paste",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "vscode:package": "pnpm vsce package --no-dependencies",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "repository": {
		"type": "git",
		"url": "https://github.com:xlei1123/limu-paste"
	},
	"bugs": {
		"url": "https://github.com/xlei1123/limu-paste/issues"
	},
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.72.2",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@dking/dgit": "^0.0.12"
  }
}
